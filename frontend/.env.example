# Frontend Environment Variables
# These are used by the frontend at build/runtime

# API Configuration
VITE_API_BASE_URL=https://your-api-gateway-url.amazonaws.com/Prod
VITE_AWS_REGION=us-east-1

# App Configuration
VITE_APP_NAME=CloudOps Assistant
VITE_VERSION=1.0.0
VITE_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_DRIFT_DETECTION=true
VITE_ENABLE_COST_DASHBOARD=true
VITE_ENABLE_AI_FEATURES=true

# GitHub Configuration (for frontend repo scanning)
VITE_GITHUB_TARGETS=your-github-username

# Security
VITE_MAX_FILE_SIZE=10485760
